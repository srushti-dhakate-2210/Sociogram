{
   "version": 2,
   "name": "social-media",
   "builds": [
      { "src": "index.js", "use": "@vercel/node" }
   ],
   "routes": [
      { "src": "/(.*)", "dest": "/index.js" }
   ]
}


{
    "version": 2,
    "name": "social-media",
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        },
        {
            "src": "public/**",
            "use": "@vercel/static"
        }
    ],
    "routes":[
        {
            "src": "/(.*)",
            "dest": "server.js"
        },
        {
            "src": "/(.+)",
            "dest": "public/images/$1"
        }
    ]
}
