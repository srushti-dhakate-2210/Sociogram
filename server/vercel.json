{
    "version": 2,
    "name": "music-album",
    "builds": [
       { "src": "index.js", "use": "@vercel/node" },
       { "src": "public/images/", "use": "@vercel/static" }

    ],
    "routes": [
	   { "src": "/(.*)",  "dest": "/index.js" },
	   { "src": "/images/(.*)", "methods": ["GET"], "dest": "/public/images/$1" }
    ]
}
